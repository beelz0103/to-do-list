{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": ["airbnb-base", "prettier"],
  "overrides": [],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "rules": {
    "radix": ["error", "as-needed"],
    "no-console": "off",
    "quotes": ["error", "double"],
    "eqeqeq": ["error", "always"]
  }
}
